{
  "name": "ewal-net-games",
  "version": "1.0.0",
  "description": "Board games page on games.ewal.net",
  "repository": {
    "type": "git",
    "url": "https://github.com/ervwalter/games.ewal.net.git"
  },
  "scripts": {
    "start": "run-p dotnet-run webpack-dev-server",
    "build": "run-p dotnet-build webpack-dev",
    "deploy": "run-s webpack-prod dotnet-publish git-add git-commit git-push",
    "webpack-dev": "webpack",
    "webpack-prod": "webpack -p --env prod",
    "webpack-dev-server": "webpack-dev-server --open --hot",
    "dotnet-build": "dotnet build",
    "dotnet-publish": "dotnet publish -o .publish",
    "dotnet-run": "dotnet run",
    "git-add": "git -C .publish add --all",
    "git-commit": "git -C .publish commit -m autocommit",
    "git-push": "git -C .publish push"
  },
  "author": "Erv Walter <erv@ewal.net> (http://github.com/ervwalter)",
  "license": "MIT",
  "homepage": "https://games.ewal.net",
  "devDependencies": {
    "@types/core-js": "^0.9.35",
    "@types/lodash": "^4.14.52",
    "@types/react": "^15.0.6",
    "@types/react-dom": "^0.14.22",
    "@types/whatwg-fetch": "0.0.33",
    "awesome-typescript-loader": "^3.0.0-beta.18",
    "clean-webpack-plugin": "^0.1.15",
    "concurrently": "^3.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.0.0-rc.2",
    "handlebars": "^4.0.6",
    "handlebars-loader": "^1.4.0",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.0",
    "npm-run-all": "^4.0.1",
    "react-hot-loader": "^1.3.1",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "ts-loader": "^2.0.0",
    "typescript": "^2.4.2",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.2.1",
    "webpack-livereload-plugin": "^0.10.0"
  },
  "dependencies": {
    "bulma": "^0.5.0",
    "core-js": "^2.4.1",
    "json-fetch": "^3.1.0",
    "lodash": "^4.17.4",
    "mobx": "^3.0.2",
    "mobx-react": "^4.1.0",
    "mobx-react-devtools": "latest",
    "mobx-utils": "^2.0.1",
    "moment": "^2.17.1",
    "react": "latest",
    "react-dom": "latest",
    "whatwg-fetch": "^2.0.2"
  }
}
